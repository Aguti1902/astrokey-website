{
  "version": 2,
  "name": "astrokey",
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/admin/index.html"
    },
    {
      "source": "/intro",
      "destination": "/intro.html"
    },
    {
      "source": "/payment",
      "destination": "/payment.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/results",
      "destination": "/results.html"
    },
    {
      "source": "/test",
      "destination": "/test.html"
    },
    {
      "source": "/email-request",
      "destination": "/email-request.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/admin/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "env": {
    "DOMAIN": "astrokey.io",
    "ENVIRONMENT": "production"
  },
  "errorPage": "/404.html"
}
